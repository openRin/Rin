{
  "name": "rin",
  "version": "1.0.0",
  "workspaces": [
    "server",
    "client"
  ],
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:client": "bun --filter './client' dev",
    "dev:server": "bun wrangler dev --port 11498",
    "check": "turbo check",
    "cf-deploy": "bun server/migrator.ts",
    "b": "turbo build",
    "t": "turbo t",
    "g": "turbo run g",
    "m": "bun scripts/dev-migrator.ts",
    "i18n": "bun --filter './client' i18n",
    "clean": "rm -rf dist node_modules client/node_modules server/node_modules",
    "format:check": "turbo format:check",
    "format:write": "turbo format:write"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "i18next-parser": "^9.0.0",
    "postcss": "^8.4.38",
    "puppeteer": "^22.10.0",
    "tailwindcss": "^3.4.3",
    "turbo": "^1.13.3",
    "vitest": "1.3.0"
  },
  "dependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "fast-xml-parser": "^4.4.0",
    "html-to-md": "^0.8.5",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-katex": "^7.0.0",
    "rehype-mathjax": "^6.0.0",
    "rehype-parse": "^9.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-react": "^8.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-rehype": "^11.1.0"
  }
}
