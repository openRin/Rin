{
  "name": "rin",
  "version": "1.0.0",
  "workspaces": [
    "server",
    "client"
  ],
  "private": true,
  "scripts": {
    "dev": "bun scripts/dev.ts",
    "dev:setup": "bun scripts/setup-dev.ts",
    "dev:client": "bun --filter './client' dev",
    "dev:server": "bun wrangler dev --port 11498",
    "dev:cron": "bun wrangler dev --port 11498 --test-scheduled",
    "db:migrate": "bun scripts/db-migrate-local.ts",
    "db:generate": "bun --filter './server' db:gen",
    "db:fix": "bun scripts/db-fix-top-field.ts",
    "typecheck": "turbo check",
    "deploy": "bun scripts/deploy-cf.ts",
    "build": "turbo build",
    "i18n:extract": "bun --filter './client' i18n",
    "clean": "rm -rf dist node_modules client/node_modules server/node_modules .turbo wrangler.toml client/.env .dev.vars",
    "format:check": "turbo format:check",
    "format:write": "turbo format:write",
    "release": "bun scripts/release.ts"
  },
  "devDependencies": {
    "turbo": "^1.13.3"
  }
}
